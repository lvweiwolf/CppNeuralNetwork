{
    "tasks": [
		{
			"type": "shell",
			"label": "Build",
			"command": "cmake",
			"args": [
				"--build",
				"${workspaceFolder}/build",
				"--config",
				"Debug",
				"--target",
				"all",
				"-j 8"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"type": "shell",
			"label": "Clean",
			"command": "mingw32-make",
			"args": [
				"clean",
				"--directory",
				"${workspaceFolder}/build",
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	],
    "version": "2.0.0"
}